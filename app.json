{
    "entryPagePath": "pages/index/index",
    "pages": [
        "pages/index/index",
        "pages/examples/examples",
        "pages/aboutme/aboutme"
    ],
    "window": {
        "navigationBarTitleText": "你好世界",
        "navigationBarBackgroundColor": "#f3514f",
        "enablePullDownRefresh": true,
        "backgroundColor": "#efefef"
    },

    "tabBar": {
        "list": [
            {
                "text": "首页",
                "pagePath": "pages/index/index",
                "iconPath": "assets/tabbar/home.png",
                "selectedIconPath": "assets/tabbar/home.png"
            },{
                "text": "示例",
                "pagePath": "pages/examples/examples",
                "iconPath": "assets/tabbar/examples.png",
                "selectedIconPath": "assets/tabbar/examples.png"
            },{
                "text": "我的",
                "pagePath": "pages/aboutme/aboutme",
                "iconPath": "assets/tabbar/aboutme.png",
                "selectedIconPath": "assets/tabbar/aboutme.png"
            }
        ]
    },
    "style": "v2",
    "componentFramework": "glass-easel",
    "sitemapLocation": "sitemap.json",
    "lazyCodeLoading": "requiredComponents",
    "usingComponents": {
        "custom-checkbox": "./components/custom-checkbox/custom-checkbox"
    },
    "requiredPrivateInfos": [
      "getLocation",
      "chooseLocation"
    ],
    "permission": {
      "scope.userLocation": {
        "desc": "你的位置信息将用于小程序位置接口的效果展示" 
      }
    },
    "subPackages": [
      {
        "root": "modules/exampleModule",
        "name": "exampleModule",
        "pages": [
          "calc/calc",
          "event/event",
          "requestapi/requestapi",
          "investigate/investigate",
          "file/file",
          "category/category",
          "compon/compon",
          "media/media",
          "memo/memo",
          "position/position",
          "cust-component/cust-component"
        ]
      }
      
    ],
    "preloadRule": {
      
    }
}
